 <!--SCRIPTS-->   
<script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/9.3.2/swiper-bundle.esm.browser.js" integrity="sha512-4Dk2Gb3sqcMi/ZHzSbKLcqhXOr43jzz4Bw6eNDSw3zh4NoEt46IvdbzWjVXL4buDB4r4zoYwQzcnoWCSOpMuoA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script>
    const swiper = new Swiper('.swiper-container', {
  // Optional parameters
  direction: 'vertical',
  loop: true,

  // If we need pagination
  pagination: {
    el: '.swiper-pagination',
  },

  // Navigation arrows
  navigation: {
    nextEl: '.swiper-button-next',
    prevEl: '.swiper-button-prev',
  },

  // And if we need scrollbar
  scrollbar: {
    el: '.swiper-scrollbar',
  },
});
</script>
